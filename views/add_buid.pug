div(class="init_dashboard")
	img(src="/static/img_main_file/abc_logo.png" class="logo_dashboard_restorant")
	div(class="menu_dashboard")

	div(class="menu_dashboard")
		a(class="red_icon_power" onclick="exit();" href="/exit")
			i.material-icons.red_icon_power power_settings_new


div(class="main_dashboar")
	div(class="personal_dates")
		i.material-icons.icons_personal_dates folder_shared
		br
		
		h5 EDITAR DATOS PERSONALES

	h5 
	i.material-icons contact_mail | 
	label Correo Electronico
	.col
		input.form-control.input_check(class="edit_my" type='mail', value=dates_user.mail name='e_m_m' id="e_m_m")
	hr
	i.material-icons account_circle | 
	label Nombre
	.col
		input.form-control.input_check(class="edit_my" type='text', value=dates_user.name name='e_m_n' id="e_m_n")
	hr
	i.material-icons person_pin | 
	label Usuario
	.col
		input.form-control.input_check(class="edit_my" type='text', value=dates_user.user name='e_m_u' id="e_m_u")
	hr
	i.material-icons contact_phone | 
	label Telefono
	.col
		input.form-control.input_check(class="edit_my" type='text', value=dates_user.phone name='e_m_p' id="e_m_p")
	a.edit_my_data.btn.float-right.login_btn.rock(onclick="edit_user_arraw('"+dates_user.user_id+"')")
		i.material-icons brush	

div(class="build_main_create")
	h5 Añade un negocio

	hr
	form(method="POST" enctype='multipart/form-data' id="upload_logo")
		.input-group.form-group
		.input-group-prepend
		span.input-group-text
			i.material-icons business
		input.form-control(type='text', placeholder="Nombre del negocio" name="business_name" id="name")

		.input-group.form-group
		.input-group-prepend
		span.input-group-text
			i.material-icons notes
		input.form-control(type='text', placeholder="A que se dedica tu negocio?" name="description_business" id="des")

		.input-group.form-group
		.input-group-prepend
		span.input-group-text
			i.material-icons business

		select(class="form-control dedicated-business" name="business_dedicated" id="dedicated")
			option Restaurante
			option Hotel
			option Bar 
			option Cafeteria
			option Negocios varios
		.input-group.form-group
		.input-group-prepend
		span.input-group-text
			i.material-icons hourglass_full
		input.form-control(type='time', placeholder="Hora de apertura del negocio" name="open_business" id="open")

		.input-group.form-group
		.input-group-prepend
		span.input-group-text
			i.material-icons hourglass_empty
		input.form-control(type='time', placeholder="Hora de cerrar del negocio" name="close_business" id="close")

		.input-group.form-group
		.input-group-prepend
		span.input-group-text
			i.material-icons near_me
		input.form-control(type='text', placeholder="Ubicacion del negocio" name="location_business" id="log")

		.input-group.form-group
		.input-group-prepend
		span.input-group-text
			i.material-icons phone
		input.form-control(type='text', placeholder="Telefono de contacto del negocio" name="phone_business" id="phone")
		input(type="hidden" value=dates_user.user_id name="user_business")

		h4 Añadir una portada a tu negocio
		hr
		span(class="logo_upload")
			span
				label(for='logo_upload') Escoge una imagen
			input(type="text" id="file_text_edit_logo")
			input(type='file', id="logo_upload" name="file" )
			input(type="hidden" name="user_image_bussines" value=dates_user.user_id id="user_business")

		div(class="start")
			button.start_bussines.btn.login_btn(onclick="start_bussines()")
				i.material-icons public 
				p Empezar a descubrir!

				