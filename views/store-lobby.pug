link(rel='StyleSheet' href="/static/style.css" type="text/css")
div(id=power class="power")

div(class="one_index")
	img(src="/static/img_main_file/logo_abancaycito_v1.png" class="ppp")
	div(class="menu2")


		a(href="/dashboard")
			i.material-icons dashboard
		div(class="options_home")
			a(class="jobs_show")
				i.material-icons business_center
			
		a(class="" onclick="exit();" href="/exit")
			i.material-icons.other_icon_power power_settings_new
	a.unreal.btn.float-right.login_btn(href="/tienda")
		i.rac.material-icons store
		p(class="rac_black") Tienda

	a.unreal_jobs.btn.float-right.login_btn(href="/trabajo")
		i.rac.material-icons library_books
		p(class="rac_black") Trabajos

div(class="principal_store")
	div(class="my_offers")
		img(src="/static/img_main_file/abc_logo.png" class="store_1")
		h1(class="store_2") -> Mis ofertas
		hr
		h3(class="store_3")
			i.material-icons style 
			p Mis Productos en venta!

		div(class="this_sold")
			a(class="back_solds")
				i.Medium.material-icons reply
			div(class="info_venta")
		div(class="xo")
			div(class="pictures")
		div(class="all_sold")
			each ventas in venta
					div(class="venta" id=ventas._id)
						img(src=ventas.principal_rute class="one_dimencion")
						div(class="fresh")
							div(class="venta_name")
								h2 #{ventas.name}
							hr
							h5 #{ventas.description}
							p(class="p_venta") #{ventas.date}
		div(class="add_picture")
			input(type="hidden" value=power id="power")
	div(class="my_jobs")
		img(src="/static/img_main_file/abc_logo.png" class="store_1")
		h1(class="store_2") -> Mis ofertas de trabajo
		hr
		h3(class="store_jobs")
			i.material-icons style 
			p Mis ofertas publicadas!

		each work in jobs
			div(class="works" id=work._id)
				div(class="title_work")
					h5 #{work.affair}
				hr
				div(class="inf_work")
					p 
						i.material-icons monetization_on
						p(class="earn") #{work.earn}
				div(class="inf_work")
					p 
						i.material-icons call
						p #{work.phone}
				div(class="inf_work")
					p 
						i.material-icons room
						p #{work.location}
				div(class="inf_work")
					p 			
						i.material-icons today
						p #{work.date}
				div(class="remove_work")
					p(onclick="remove_work('"+work._id+"')") 			
						i.material-icons delete_forever



		

script(src='/static/config.js')