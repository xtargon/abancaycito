div(class="two_dashboard")
	a(href="/")
		img(src="/static/img_main_file/abc_logo.png" class="logo_dashboard_restorant")

	div(class="build_dashboar")
		h4 bussiness Config
	div(class="orders_dashboar")
		h4 view all orders 

	div(class="menu_dashboard")
		a(href="/")
			i.material-icons home
		a(href="/tienda")
			i.material-icons shopping_cart
		a(href="/trabajo")
			i.material-icons business_center
div(class="all_view_restaurant")
	div(class="main_images")
		img(src=restaurant_dates.portada class="cover_page")
		div(class="h3")
			h3= restaurant_dates.name
		img(src=restaurant_dates.logo class="logo_page")

	div(class="details_restaurant")
		a 
			i.material-icons.icons_personal_dates alarm

			=restaurant_dates.open
		a 
			i.material-icons.icons_personal_dates alarm_off

			= restaurant_dates.close

		a 
			i.material-icons.icons_personal_dates phone_in_talk

			= restaurant_dates.phone

		a.not_line
			i.material-icons.icons_personal_dates room

			= restaurant_dates.location_business
	div(class="text_restaurant")
		h5 
			i.material-icons.icons_personal_dates sort

			= restaurant_dates.description

.slideshow-container
	each catalogo in Catalogue 
		.mySlides.fade
			img(src=catalogo.Ruta, style='width:550px; height:350px;')



	a.prev(onclick='plusSlides(-1)') ❮
	a.next(onclick='plusSlides(1)') ❯

	div(style='text-align:center')
		- var nums = 1
		each catalogo in Catalogue.length ? Catalogue : ['There are no values']
			while nums <= Catalogue.length
				span.dot(onclick='currentSlide('+nums+')')
				a(class="oculto") #{nums++}



div(class="box-input-delivery-query")
	div
		input(type="text" class="input-delivery" id="input_delivery" placeholder="¿Que quieres pedir hoy?" readonly)
	
		button(class="CHEKK" id="clean" onclick="partir()")
			p 
				i.material-icons.icons_personal_dates send
	hr
	div(class="articles_box")
		table(style='width:100%')
				
			tr
				th Producto
				th Precio 
				th Descripcion
		each Producto in product 
	

				div(class="art_new" id=Producto.id)
							
					h4(class='name_item all_item') #{Producto.name}

					p(class="des all_item") #{Producto.description}

					h5(class="price all_item") #{Producto.price}
	


	div(class="see_check")
	input(type="hidden" value=restaurant_dates._id id="this_restaurant")

div(class="orders_dashboar" id="orders_dashboard")
audio#audio(controls='')
	source(type='audio/mp3' src='/static/clin.mp3')


