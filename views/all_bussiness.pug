div(class="init_dashboard")
	img(src="/static/img_main_file/abc_logo.png" class="logo_dashboard_restorant")
	div(class="this_section")
		h3 #{inf}

	div(class="menu_dashboard")
		a(href="/")
			i.material-icons home

div(class="all_show_rest")
	each val in restaurant_dates 
		.card
			.card-image.waves-effect.waves-block.waves-light
				img.activator.img_all_show(src=val.logo)
			.card-content
				span.card-title.activator.grey-text.text-darken-4
					= val.name
					i.material-icons.right.closese more_vert
				p
					a.cls(href='/vistazo/'+val.name) Echar un vastazo
			.card-reveal
				span.card-title
					sdfsdfsdfs 

				span.card-title.blue-text.text-darken-4
					= val.name
					i.material-icons.right.closese close
				p
					=val.description
				hr

				p
					i.material-icons.icon_card phone

					=val.phone

				p
					i.material-icons.icon_card room

					=val.location_business

				p
					i.material-icons.icon_card alarm

					=val.open

				p
					i.material-icons.icon_card alarm_off

					=val.close